    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "gh-pages": "^5.0.0",
    "parcel": "^2.8.3",
    "parcel-resolver-ts-base-url": "^1.3.1",
    "parcel-resolver-tspaths": "^0.0.9",
